{
  "env": {
    "jest": true,
  },
  "plugins": ["react"],
  "extends": [
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "../../.eslintrc",
  ],
  "parserOptions": {
    "sourceType": "module",
    "project": ["./tsconfig.json"],
  },
  "settings": {
    "react": {
      "version": "detect",
    },
    "import/resolver": {
      "node": {
        "paths": ["src"],
      },
    },
  },
}
